<%- include('includes/head.ejs') %>
<%- include('includes/nav.ejs') %>
<div class="contenido">
    <h1 class="contenido-title">Nueva contraseña</h1>
    <%if(error) {%>
        <div class="card-mensaje card-mensaje--error"><%=error%></div>
        <%}%>
    <div class="card">
        <h3 class="card-title">Nueva contraseña</h3>
        <form action="<%=url%>" method="post" class="form-login">
        <label for="contraseña">Contraseña <input type="password" name="contraseña" id="contraseña" required placeholder="Ingresa tu nueva contraseña"></label>
        <label for="contraseñaR">Repetir contraseña <input type="password" name="contraseñaR" id="contraseñaR" required placeholder="Repetir tu contraseña"></label>
        <input type="hidden" name="_csrf" value="<%=csrfToken%>" />
        <input type="submit" value="Reniciar">
        
    </form>
    <input type="hidden" name="_csrf" value="<%=csrfToken%>" />    </div>
</div>
<%- include('includes/end.ejs') %>