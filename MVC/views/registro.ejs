<%- include('includes/head.ejs') %>
<%- include('includes/nav.ejs') %>
<div class="contenido">
    <h1 class="contenido-title">Catalogo de productos</h1>
    <div class="card">
        <h3 class="card-title">Login</h3>
        <form action="/registro" method="post" class="form-login">
        <label for="correo">Correo <input type="text" name="correo" id="correo" required placeholder="Ingresa tu correo"></label>
        <label for="usuario">Usuario <input type="text" name="usuario" id="usuario" required placeholder="Ingresa tu usuario"></label>
        <label for="contraseña">Contraseña <input type="password" name="contraseña" id="contraseña" required placeholder="Ingresa tu contraseña"></label>
        <label for="contraseñaR">Repite Contraseña <input type="password" name="contraseñaR" id="contraseñaR" required placeholder="repite tu contraseña"></label>
        <input type="hidden" name="_csrf" value="<%=csrfToken%>" />
        <input type="submit" value="Registrar">
    </form>
    </div>
</div>
<%- include('includes/end.ejs') %>